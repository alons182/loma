
			
@import "iconos.styl"



$naranja = #db841e
$gris = #bbc0ac
$verde = #33520e
$cafe = #736841

*:focus
	outline 0

.jspArrowDown, .mCSB_buttonDown
	background-image url('../img/flecha_down.gif') !important
	background-position 50% 50% !important
	background-repeat no-repeat
	background #cfc8c0
	width 10px

.jspArrowUp, .mCSB_buttonUp
	background-image url('../img/flecha_up.gif') !important
	background-position 50% 50% !important
	background-repeat no-repeat
	background #cfc8c0
	width 10px
body
	background #f0efed
	color #333
	font normal 14px arial, sans-serif
	height 100%
	margin 0
	overflow-y scroll
	padding 0
	position relative

#main_header
	background #222
	color #fff
	display block
	height 72px
	position relative

	a 
		color #fff

	#logo
		position absolute
		top 0
		left 0
	#btn_nav
		display none
	#menu
		margin-left 130px
		font italic normal 15px Georgia,serif
		display block

		ul 
			list-style-type none
			margin 0
			padding 0 0 0 25px

			li 
				display inline-block
				vertical-align top

				a, span 
					display block
					background url('../img/icono_nav.png') 15px 31px no-repeat
					background-size 13px 12px
					padding 28px 15px 22px 35px
					height 22px
					outline none
					position relative
					text-decoration none
				a:hover, span:hover
					background-image url('../img/icono_nav_naranja.png')
				
			.active a, .active span
				background-image url('../img/icono_nav_naranja.png')
				background-color #fff
				color #000

	.idiomas
		position absolute
		right 20px
		top 30px
		font normal 12px Georgia,serif

		a 
			background url('../img/icono_circulo_blanco.png') 0 0 no-repeat
			padding-left 26px
			padding-bottom 5px
			padding-top 3px

#main 
	position relative

	#menu_proyecto
		position absolute
		top 0px
		z-index 1
		background #f9f5f1
		-moz-box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)
		-webkit-box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)
		box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)

		ul
			margin 0
			padding 0

			li 
				
				border-bottom 1px solid #d7d1c4 

				a
					display block
					text-transform uppercase
					font bold 24px Georgia,serif
					text-align center
					padding 1em
					color #000
					text-decoration none
					cursor pointer

					span 
						display block
						font-size 14px 
						font-style italic
						font-weight normal
				a:hover
					background $naranja
					background-image -webkit-linear-gradient(top, #fb9114, #c1700b)
					background-image -o-linear-gradient(top, #fb9114, #c1700b)
					background-image linear-gradient(to bottom, #fb9114, #c1700b)
					color #fff
					text-shadow 0 2px 0 rgba(0, 0, 0, 0.5)
					
					


	#slider
		
		.slide
			overflow hidden
			position relative
		.cir_banner
			position absolute
			top 10%
			left 53%
			background url('../img/cir_banner.png') right 0 no-repeat
			width 262px
			height 214px
			padding 70px 60px 90px 58px
			color #fff
			text-align center
			text-decoration none

			span
				display block
			.h2
				font italic bold 36px/36px Georgia,serif
				text-transform uppercase
				border-bottom 1px dotted #fff
				padding-bottom 9px
				margin 14px 0 9px 0
			.p 
				font normal 1.7em Georgia,serif
				margin 0

	.franja
		display block
		position absolute
		bottom 0
		left 0
		height 174px
		width 100%
		background url('../img/textura_franja.png') 50% 0 repeat
		text-align center
		color #fff

		h1
			
			font normal 75px Georgia,serif
			text-transform uppercase
			margin 0
			padding-top 15px
		
		.linea
			background $naranja
			height 5px
			margin 5px 0
		span
			display block
			font italic normal 15px Georgia,serif

	#contacto, #plano
		zoom: 1

		.columna
			float left
			width 420px
			background  #eaece3
			-moz-box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)
			-webkit-box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)
			box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)

			h1
				font normal 40px  Georgia,serif
				color #333
				margin 30px 0 20px 20px
				text-transform uppercase

			.inner 
				padding 20px 0 20px 20px

			.form-item:after 
				clear both
			.form-item:before, .form-item:after 
				content ""
				display table
			.form-item
				margin-top 5px
				zoom: 1
			
			
				
				
				label
					font normal 14px Georgia,serif
					color #333
					width 130px
					float left
					display block
					text-align right
					margin-right 10px
					padding-top 10px

				.form-text
					border-top none
					border-left none
					border-right none
					border-bottom 1px solid #b2b1af
					padding 10px 0 8px 0
					font normal 15px Georgia,serif
					color #999
					background none
					width 220px
					display block
					float left
				.form-text:hover, .form-text:focus, textarea:hover, textarea:focus
					background #f9f5f1
					border-color #dd841d
				textarea
					border-top none
					border-left none
					border-right none
					border-bottom 1px solid #b2b1af
					padding 10px 0 8px 0
					font normal 15px Georgia,serif
					color #999
					background none
					width 220px
					height 100px
					display block
					float left
					resize none

			.buttons
				width 360px
				margin-top 10px

				button,input[type="submit"]
					background url('../img/icono_circulo_blanco.png') 10px 10px no-repeat  $naranja
					border none
					clear both
					color white
					cursor pointer
					float right
					font-size 12px
					height 40px
					margin-right 4px
					width 117px
					text-transform uppercase

			.marca
				background url('../img/marca.png') center center no-repeat
				color #fff
				font italic normal 12px Georgia,serif
				width 290px
				height 235px
				padding 0 45px
				margin-top 20px
			.marca2
				background url('../img/marca2.png') center center no-repeat
				color #fff
				font italic normal 12px Georgia,serif
				width 290px
				height 235px
				padding 0 45px
				margin-top 20px
				


		.plan
			float left

			.info-top
				background url('../img/textura_franja.png') 50% 0 repeat
				height 50px
				color #fff

				a 
					color #fff

				
				h2
					padding-top 0.6em
					padding-left 1em
					margin 0
					font italic 18px Georgia,serif
	#plano
		p 
			font italic normal 18px Georgia,serif

		.info-lotes
			background #fff
			padding 1em
			width 320px
			-moz-box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)
			-webkit-box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)
			box-shadow 0 5px 5px rgba(0,0,0,0.5),0 0 5px rgba(0,0,0,0.5),inset 0 0 1px 1px rgba(255,255,255,0.2),inset 0 1px 0 rgba(255,255,255,0.2),inset 0 -1px 2px rgba(0,0,0,0.05)

			h3
				margin-top 0
				font bold 24px Georgia,serif
			span
				display block
				font normal 18px Georgia,serif
		#mapa 
			position relative

			.overlay-proyecto
				position absolute
				top 0
				left 0
				right 0
				bottom 0
				background-color rgba(0, 0, 0, 0.7)
				z-index 1
				color white
				display none
				overflow hidden

				.close
					color #fff
					text-decoration none 
					display block
					background url('../img/icono_circulo_blanco.png') 3px 3px no-repeat
					padding-left 26px
					padding-right 5px
					padding-bottom 5px
					padding-top 5px
					float right
					border 1px solid #FFF
					margin-right 10px
					margin-top 10px

			.mapa-proyecto
				background url('../img/plano.jpg') 0 0 no-repeat
				width 100%
				height 100%	
				

				.lote
					display block
					cursor pointer
					position relative

					.info-item
						display none
				
				#lote-1
					background url('../img/lote.png') 0 0 no-repeat
					width 120px
					height 100px
					top 161px
					left 268px

				#lote-2
					background url('../img/lote2.png') 0 0 no-repeat
					width 120px
					height 112px
					top 151px
					left 178px
				
				#lote-1:hover
					background-position bottom center
				#lote-2:hover
					background-position 0 -126px
			
			.details-item
				//margin 20px auto 
				text-align center
				//position absolute
				//top 100px
				//right 0px
				//z-index 3
				.image-item
					position relative
					width 710px
					margin 20px auto

					#prev
						position absolute
						left -110px
						top 200px
						background url('../img/icono_prev.png') 0 0 no-repeat
						display block
						padding 3px 24px
						color #fff
						text-decoration none
					#next
						position absolute
						right -110px
						top 200px
						background url('../img/icono_next.png') 0 0 no-repeat
						display block
						padding 3px 24px
						color #fff
						text-decoration none
						
					#slideshow-nav
						left 50%
						margin-left -470px
						margin-top 389px
						position absolute
						text-align right
						width 940px
						z-index 50


						a
							background url("../img/heroes-nav.png") no-repeat scroll left top transparent
							display inline-block
							height 12px
							overflow hidden
							text-indent 999px
							width 12px

						a.activeSlide
							background-position right top
				  
					#slideshow
						height 315px
						margin 0 auto
						width 100%


						.slide
							background-position center top
							background-repeat no-repeat
							height 100%
							text-align center
							width 100%

				.detail-info
					background-color rgba(0, 0, 0, 0.7)
					//margin-top 20px
				.panel
					display inline-block
					vertical-align top
					text-align left
					margin-right 10px

					h3 
						color $naranja
						font bold 18px Georgia,serif
					ul li
						font italic 14px Georgia,serif
				
		


#main_footer 
	position relative
	background #f9f5f1
	height 46px
	overflow hidden
	display block
	clear both

	#copyright
		float left
		margin-left 11px
		margin-top 11px
		position relative
	#redes
		position absolute
		right 12px
		top 9px
		font italic normal 15px Georgia,serif
		color #95897c

		.label
			float left
			padding-right 3px
			padding-top 5px

		a 
			display inline-block
			vertical-align top
			margin-left: 5px;
			background $naranja
			color #f9f5f1
			text-decoration none
			padding .5em



@import "responsive.styl"
@import "mCustomScrollbar.styl"
					

